<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Gradient background -->
    <path
        android:fillColor="#004D40"
        android:pathData="M0,0h108v108h-108z" />

    <!-- Gradient overlay -->
    <path
        android:pathData="M0,0h108v108h-108z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:type="radial"
                android:centerX="54"
                android:centerY="54"
                android:gradientRadius="80"
                android:startColor="#00897B"
                android:centerColor="#00695C"
                android:endColor="#004D40"
                android:tileMode="clamp" />
        </aapt:attr>
    </path>

    <!-- Subtle pattern overlay -->
    <path
        android:pathData="M0,0h108v108h-108z"
        android:fillColor="#05FFFFFF" />

    <!-- Accent corner -->
    <path
        android:pathData="M0,0 L40,0 L0,40z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:type="linear"
                android:startX="0"
                android:startY="0"
                android:endX="40"
                android:endY="40"
                android:startColor="#F57C73"
                android:endColor="#00F57C73"
                android:tileMode="clamp" />
        </aapt:attr>
    </path>
</vector>
